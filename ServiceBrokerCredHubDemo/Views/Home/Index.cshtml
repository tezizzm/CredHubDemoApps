@model IList<CityViewModel>

@{
    ViewData["Title"] = "Home Page";
}

<table class="table">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">City</th>
    </tr>
    </thead>
    <tbody>
    
    @foreach (var city in Model)
    {
        <tr>
            <th scope="row">@city.Id</th>
            <td>@city.CityName</td>
        </tr>
    }
    </tbody>
</table>
