{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "Steeltoe.Security.DataProtection.CredHub": "Trace"
    }
  },
  "management": {
    "endpoints": {
      "path": "/cloudfoundryapplication",
      "cloudfoundry": {
        "validateCertificates": false
      }
    }
  },
  "CredHubClient": {
    "ValidateCertificates": "false",
    "clientId": "CredHubDemoId",
    "clientSecret": "CredHubDemoSecret"
  }
}
